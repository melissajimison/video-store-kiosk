<div class="movies">
{{#each model as |movie|}}
<div class="movie">
  <img src="http://localhost:3000/images/posters/{{movie.poster_filename}}">
</div>
  <div class="info">
       <h2>{{movie.title}} </h2>
       {{movie.release_date}}
       <br>
       {{movie.overview}}
       <br>
       {{movie.inventory}}
       <br>
       {{movie.poster_filename}}
       {{movie.createdAt}}
       {{movie.updatedAt}}
     </div>
{{/each}}


</div>
